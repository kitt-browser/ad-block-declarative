{
  "manifest_version": 2,
  "name": "AdBlockDeclarative",
  "version": "0.5.1",
  "description": "AdBlockerDeclarative is an ad blocking extension based on chrome.declarativeWebRequest",
  "author": "Jan Dedecek",
  "icons": {
    "256": "images/shield.png"
  },
  "content_scripts": [
    {
      "js": [
        "bower_components/jquery/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
